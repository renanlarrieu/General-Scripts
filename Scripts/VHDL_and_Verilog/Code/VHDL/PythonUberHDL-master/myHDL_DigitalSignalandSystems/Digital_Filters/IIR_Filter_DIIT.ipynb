   "source": [
    "## myHDL Simulation Post Processing and Reduction \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "metadata": {},
   "outputs": [],
   "source": [
    "#create signals and bind to Peeker\n",
    "Peeker.clear()\n",
    "x=Signal(intbv(0, min=Min2s, max=Max2s)); Peeker(x, 'x')\n",
    "y=Signal(intbv(0, min=Min2s, max=Max2s)); Peeker(y, 'y')\n",
    "clk, rst=[Signal(bool(0)) for _ in range(2)]\n",
    "Peeker(clk, 'clk'); Peeker(rst, 'rst')\n",
    "\n",
    "#instantiate the Delay as the DUT\n",
    "DUT=Delay(x, y, clk, rst)\n",
    "\n",
    "\n",
    "def Delay_TB():\n",
    "    \n",
    "    #clock generator\n",
    "    @always(delay(1))\n",
    "    def clkGen():\n",
    "        clk.next=not clk\n",
    "    \n",
    "    #the test stimulus\n",
    "    @instance\n",
    "    def stimulus():\n",
    "        # run inputs from the DF used in the pure python testing\n",
    "        # to then compare to in post processing of test\n",
    "        for _, inputs in DelayTestDF.iterrows():\n",
    "            yield clk.posedge\n",
    "            x.next,_, _=inputs\n",
    "            \n",
    "        # Test the reset\n",
    "        for i in range(4):\n",
    "            if i%2==0:\n",
    "                x.next=1; L.next=1\n",
    "                rst.next=1\n",
    "            else:\n",
    "                x.next=1; L.next=1\n",
    "                rst.next=0\n",
    "            yield clk.posedge\n",
    "        \n",
    "                \n",
    "        # Completed test inputs and terminating simulation    \n",
    "        raise StopSimulation\n",
    "\n",
    "    return instances()"
   ]
  },
