    "@block\n",
    "def DIITU(x, y, L, U, a, b, clk, rst):\n",
    "    \n",
    "    #the sum before delay\n",
    "    #Sum=Signal(modbv(0)[BitWidth:])\n",
    "    \n",
    "    @always(clk.posedge)\n",
    "    def logic():\n",
    "        if rst:\n",
    "            U.next=0\n",
    "        else:\n",
    "            #call the last val in Sum to act as delay\n",
    "            #U.next=Sum\n",
    "            #Sum input\n",
    "            U.next=L+x*b-y*a\n",
    "    return instances()\n",
    "        "
   ]
