   "source": [
    "## Implementation using pure python as ref"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "metadata": {},
   "outputs": [],
   "source": [
    "class DIITREf():\n",
    "    def __init__(self,a, b):\n",
    "        self.a=a; self.b=b\n",
    "        self.S=[]\n",
    "        self.ZRet=[0]\n",
    "        \n",
    "    def Add(self, x, y, L):\n",
    "        self.S.append(-self.a*y +self.b*x +L)\n",
    "    \n",
    "    def Delay(self):\n",
    "        self.ZRet.append(self.S[-1])\n",
    "        self.R=self.ZRet[-2]\n",
    "    \n",
    "    def Action(self,x, y, L):\n",
    "        self.Add(x, y, L)\n",
    "        self.Delay()\n",
    "        return self.R"
   ]
  },
