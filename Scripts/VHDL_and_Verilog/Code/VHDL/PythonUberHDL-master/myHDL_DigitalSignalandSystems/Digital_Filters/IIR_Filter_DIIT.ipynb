    "## myHDL implementation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "metadata": {},
   "outputs": [],
   "source": [
    "@block\n",
    "def Delay(x, y, clk, rst):\n",
    "    '''\n",
    "        Z delay bulding block for a CIC filter\n",
    "        \n",
    "        Inputs:\n",
    "            x (but): the x(n) data in feed\n",
    "\n",
    "            -----------------------\n",
    "            \n",
    "            clk(bool): clock feed\n",
    "            rst(bool): reset feed\n",
    "        \n",
    "        Outputs:\n",
    "            y (data):  the y(n+1) output of y(n+1)=x(n)\n",
    "            \n",
    "    '''\n",
    "    @always(clk.posedge)\n",
    "    def logic():\n",
    "        if rst:\n",
    "            y.next=0\n",
    "        else:\n",
    "            y.next=x\n",
    "    return logic"
   ]
  },
