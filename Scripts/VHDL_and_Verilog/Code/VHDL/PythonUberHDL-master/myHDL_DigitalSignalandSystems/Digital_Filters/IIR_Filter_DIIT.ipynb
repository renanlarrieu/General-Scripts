    "def IIRP_HC(x, y, clk, rst, a, b):\n",
    "    \"\"\"\n",
    "    Prototyp DIIT 2nd Order with hardcoded coeff\n",
    "    This Prototype does autowiring to order 3\n",
    "    Note:\n",
    "        in order to move on will need to confirm that \n",
    "        concat was fixed in myhdl .10\n",
    "        \n",
    "        \n",
    "    \"\"\"\n",
    "    \n",
    "    N=len(b)\n",
    "    \n",
    "    #create the wireing to section i from section j\n",
    "    #ij =to, from\n",
    "    Sec_ij=[Signal(intbv(0, min=Min2s, max=Max2s))  for _ in range(N)]\n",
    "  \n",
    "    \n",
    "    Sec=[None for _ in range(N)]\n",
    "    for i in range(4):\n",
    "        if i==0:\n",
    "            Sec[i]=DIITC(x, y, Sec_ij[0], b[i], clk, rst)\n",
    "        else:\n",
    "            # the leading 1 (True a_0) was hard encoded into the Cap \n",
    "            Sec[i]=DIITU(x, y, Sec_ij[i], Sec_ij[i-1], a[i-1], b[i], clk, rst)\n",
    "        \n",
    "    \n",
    "    return instances()\n",
